     item.classList.toggle('hover:bg-gray-100', index !== selectedIndex);\n    });\n  };\n\n  let selectedLink = null;\n\n  const navigateResults = (e) => {\n    const items = instantResults.querySelectorAll('.result-item');\n    console.log('⭐️ items.length', items.length);\n    console.log(\n      \"⭐️ instantResults.classList.contains('hidden')\",\n      instantResults.classList.contains('hidden'),\n    );\n    // if (!items.length || instantResults.classList.contains('hidden')) return;\n\n    if (e.key === 'ArrowDown') {\n      e.preventDefault();\n      selectedIndex = Math.min(selectedIndex + 1, items.length - 1);\n      selectedLink = items[selectedIndex]?.querySelector('a');\n      console.log(selectedLink);\n      updateHighlight();\n    } else if (e.key === 'ArrowUp') {\n      e.preventDefault();\n      selectedIndex = Math.max(selectedIndex - 1, -1);\n      selectedLink = items[selectedIndex]?.querySelector('a');\n      console.log(selectedLink);\n      updateHighlight();\n    } else if (e.key === 'Enter') {\n      e.preventDefault(); // Prevent form submission\n      e.stopPropagation();\n      console.log('🟢', selectedLink);\n      // console.log(selectedIndex)\n      // if (selectedIndex >= 0) {\n      //   const selectedLink = items[selectedIndex].querySelector('a');\n      //   console.log(selectedLink)\n      if (selectedLink) {\n        window.location.href = selectedLink; // Navigate to selected link\n        resetResults();\n        // }\n      } else {\n        searchForm.submit(); // Fallback to form submission if no result selected\n      }\n    }\n  };\n\n  const resetResults = () => {\n    instantResults.innerHTML = '';\n    instantResults.classList.add('hidden');\n    spinnerIcon.style.display = 'none';\n    searchIcon.style.display = 'block';\n    selectedIndex = -1;\n  };\n\n  // Event listeners\n  searchInput.addEventListener('keydown', (e) => {\n    const query = e.target.value.trim();\n\n    console.log(query);\n\n    if (query.length 